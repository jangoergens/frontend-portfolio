{
	"name": "top-comment-finder",
	"version": "0.1.0",
	"scripts": {
		"start": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"install-test-browser": "playwright install chromium",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path ../../.prettierignore --check . && eslint . --ignore-path ../../.eslintignore --report-unused-disable-directives --max-warnings 0",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@playwright/test": "1.39.0",
		"@sveltejs/adapter-auto": "2.1.1",
		"@sveltejs/kit": "1.27.3",
		"@typescript-eslint/eslint-plugin": "6.9.1",
		"@typescript-eslint/parser": "6.9.1",
		"autoprefixer": "10.4.16",
		"eslint": "8.53.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-perfectionist": "2.2.0",
		"eslint-plugin-svelte": "2.34.1",
		"postcss": "8.4.31",
		"prettier": "3.0.3",
		"prettier-plugin-svelte": "3.0.3",
		"prettier-plugin-tailwindcss": "0.5.6",
		"supabase": "1.110.1",
		"svelte": "4.2.2",
		"svelte-check": "3.5.2",
		"svelte-preprocess": "5.0.4",
		"tailwindcss": "3.3.5",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite": "4.5.0",
		"vite-imagetools": "6.2.3"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "2.38.4"
	}
}
