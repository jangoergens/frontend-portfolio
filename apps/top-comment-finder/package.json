{
	"name": "top-comment-finder",
	"version": "0.1.0",
	"scripts": {
		"start": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"install-test-browser": "playwright install chromium",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path ../../.prettierignore --check . && eslint . --ignore-path ../../.eslintignore --report-unused-disable-directives --max-warnings 0",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@playwright/test": "1.41.1",
		"@sveltejs/adapter-auto": "3.1.1",
		"@sveltejs/kit": "2.5.0",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"@typescript-eslint/eslint-plugin": "6.20.0",
		"@typescript-eslint/parser": "6.20.0",
		"autoprefixer": "10.4.17",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-perfectionist": "2.5.0",
		"eslint-plugin-svelte": "2.35.1",
		"postcss": "8.4.33",
		"prettier": "3.2.4",
		"prettier-plugin-svelte": "3.1.2",
		"prettier-plugin-tailwindcss": "0.5.11",
		"supabase": "1.136.3",
		"svelte": "4.2.9",
		"svelte-check": "3.6.3",
		"svelte-preprocess": "5.1.3",
		"tailwindcss": "3.4.1",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"vite": "5.0.12",
		"vite-imagetools": "6.2.3"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "2.39.3"
	}
}
