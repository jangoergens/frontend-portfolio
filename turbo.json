{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["check", "^build"],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },
    "install-test-browser": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "dependsOn": ["^build", "^install-test-browser"],
      "outputs": [],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "check": {
      "outputs": [],
      "cache": false
    },
    "start": {
      "cache": false
    }
  }
}
