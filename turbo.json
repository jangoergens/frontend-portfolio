{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["check", "^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": [
        "GOOGLE_API_MODE",
        "PUBLIC_SUPABASE_URL",
        "PUBLIC_SUPABASE_ANON_KEY"
      ]
    },
    "install-test-browser": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "dependsOn": ["^build", "^install-test-browser"],
      "outputs": [],
      "env": [
        "GOOGLE_API_MODE",
        "PUBLIC_SUPABASE_URL",
        "PUBLIC_SUPABASE_ANON_KEY"
      ]
    },
    "lint": {
      "outputs": []
    },
    "check": {
      "outputs": [".svelte-kit/**"],
      "env": [
        "GOOGLE_API_MODE",
        "PUBLIC_SUPABASE_URL",
        "PUBLIC_SUPABASE_ANON_KEY"
      ]
    },
    "start": {
      "cache": false
    }
  }
}
