{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "check",
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "install-test-browser": {
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^build",
        "^install-test-browser"
      ],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "check": {
      "outputs": []
    },
    "start": {
      "cache": false
    }
  }
}
